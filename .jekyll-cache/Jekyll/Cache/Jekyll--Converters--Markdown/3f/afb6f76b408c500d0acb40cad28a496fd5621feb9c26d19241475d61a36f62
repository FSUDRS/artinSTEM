        var facets = Array.from(new Set(['simulation', 'dslr', 'dslr', 'dslr', 'microscope', 'cell-phone-camera', 'cell-phone-camera', 'dslr', 'microscope', 'dslr', 'cell-phone-camera', 'dslr', 'microscope', 'dslr', 'cell-phone-camera', 'dslr', 'microscope', 'dslr', 'other', 'other', 'dslr', 'cell-phone-camera', 'cell-phone-camera', 'dslr', 'cell-phone-camera', 'microscope']));
        buttonDiv.append("<button class='btn facet active' data-filter='all'>show all</button>");

        for (i in facets) {
          buttonDiv.append(`<button class='btn facet' data-filter='${facets[i]}'>${facets[i]}</button>`);
        }

        $(document).ready(function(){
          $(".facet").click(function(){
            var filterValue = $(this).attr('data-filter');
            $('.facet').removeClass('active');
            $(this).addClass('active');
            if(filterValue == 'all') { $('.all').show('slow'); }
            else { $('.all').hide('slow'); $(`.${filterValue}`).show('slow'); }
          });
        });
      
    
  });
</script>

